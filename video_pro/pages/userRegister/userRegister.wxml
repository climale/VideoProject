<view>
  <view class="login-icon">
  <image class="login-img" src="../../res/images/login.jpg"></image>
  </view>
  <view class="login-from"> 
    <form bindsubmit="doregist">
      <view class="inputView">
        <image class="nameImage" src="../../res/images/username.png"></image>
        <label class="loginLabel">账号</label>
        <input name="username" class="inputText" placeholder="请输入账号"></input>
      </view>
        <view class="inputView">
        <image class="keyImage" src="../../res/images/username.png"></image>
        <label class="loginLabel">密码</label>
        <input name="password" class="inputText" password="true" placeholder="请输入密码"></input>
      </view>
        <view >
           <button class="loginBtn" type="primary" form-type="submit">注册</button>
       </view>
      <view>
         <button class="goLoginBtn" type="warn" bindtap="goLoginPage">返回登录</button>
      </view>
    </form>

  </view>
</view>